<% if (title) { %>
  <h1 id="management-title"><%= title %></h1>
<% } %>

 <%- messages() %>

 <% if (errors) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>

<div id="management-container">
    <a class="management-a" href="add-classification">Add a new Classification</a>
    <a class="management-a" href="add-inventory">Add a new Vehicle</a>
</div>

<h2 id="inv-management-h2">Manage Inventory</h2>
<p id="inv-management-p">Choose a classification to see those inventory items</p>
<div id="inv-management-select-container">
  <%- typeof classificationSelect  !== 'undefined' ? classificationSelect : '' %>
</div>
<table id="inventoryDisplay"></table>

<noscript>JavaScript must be enabled to use this page.</noscript>

<script src="../../js/inventory.js"></script>